import time
import sys

player1 = input("Player 1: ")
player2 = input("Player 2: ")
for i in range(50):
    print("")

player1_color = "\033[94m0\033[0m"
player2_color = "\033[91m0\033[0m"

a = " "
a1 = " "
a2 = " "
a3 = " "
a4 = " "
a5 = " "
b = " "
b1 = " "
b2 = " "
b3 = " "
b4 = " "
b5 = " "
c = " "
c1 = " "
c2 = " "
c3 = " "
c4 = " "
c5 = " "
d = " "
d1 = " "
d2 = " "
d3 = " "
d4 = " "
d5 = " "
e = " "
e1 = " "
e2 = " "
e3 = " "
e4 = " "
e5 = " "
f = " "
f1 = " "
f2 = " "
f3 = " "
f4 = " "
f5 = " "
g = " "
g1 = " "
g2 = " "
g3 = " "
g4 = " "
g5 = " "

def printBoard(a5, a4, a3, a2, a1, a, b5, b4, b3, b2, b1, b, c5, c4, c3, c2, c1, c, d5, d4, d3, d2, d1, d, e5, e4, e3, e2, e1, e, f5, f4, f3, f2, f1, f, g5, g4, g3, g2, g1, g):
    print(f'''
   1     2     3     4     5     6     7
|-----|-----|-----|-----|-----|-----|-----|
|  {a}  |  {b}  |  {c}  |  {d}  |  {e}  |  {f}  |  {g}  |
|-----|-----|-----|-----|-----|-----|-----|
|  {a1}  |  {b1}  |  {c1}  |  {d1}  |  {e1}  |  {f1}  |  {g1}  |
|-----|-----|-----|-----|-----|-----|-----|
|  {a2}  |  {b2}  |  {c2}  |  {d2}  |  {e2}  |  {f2}  |  {g2}  |
|-----|-----|-----|-----|-----|-----|-----|
|  {a3}  |  {b3}  |  {c3}  |  {d3}  |  {e3}  |  {f3}  |  {g3}  |
|-----|-----|-----|-----|-----|-----|-----|
|  {a4}  |  {b4}  |  {c4}  |  {d4}  |  {e4}  |  {f4}  |  {g4}  |
|-----|-----|-----|-----|-----|-----|-----|
|  {a5}  |  {b5}  |  {c5}  |  {d5}  |  {e5}  |  {f5}  |  {g5}  |
|-----|-----|-----|-----|-----|-----|-----|
''')

def p1ask(a5, a4, a3, a2, a1, a, b5, b4, b3, b2, b1, b, c5, c4, c3, c2, c1, c, d5, d4, d3, d2, d1, d, e5, e4, e3, e2, e1, e, f5, f4, f3, f2, f1, f, g5, g4, g3, g2, g1, g):
    ask1 = int(input(f"{player1}, choose a column to drop the disk: "))
    if ask1 == 1:
        if a5 == " ":
            a5 = player1_color
            printBoard(a5, a4, a3, a2, a1, a, b5, b4, b3, b2, b1, b, c5, c4, c3, c2, c1, c, d5, d4, d3, d2, d1, d, e5, e4, e3, e2, e1, e, f5, f4, f3, f2, f1, f, g5, g4, g3, g2, g1, g)
            sys.settrace(p2ask(a5, a4, a3, a2, a1, a, b5, b4, b3, b2, b1, b, c5, c4, c3, c2, c1, c, d5, d4, d3, d2, d1, d, e5, e4, e3, e2, e1, e, f5, f4, f3, f2, f1, f, g5, g4, g3, g2, g1, g))
        if a5 == player1_color or player2_color:
            if a4 == " ":
                a4 = player1_color
                printBoard(a5, a4, a3, a2, a1, a, b5, b4, b3, b2, b1, b, c5, c4, c3, c2, c1, c, d5, d4, d3, d2, d1, d, e5, e4, e3, e2, e1, e, f5, f4, f3, f2, f1, f, g5, g4, g3, g2, g1, g)
