                                sys.settrace(p2ask(a5, a4, a3, a2, a1, a, b5, b4, b3, b2, b1, b, c5, c4, c3, c2, c1, c, d5, d4, d3, d2, d1, d, e5, e4, e3, e2, e1, e, f5, f4, f3, f2, f1, f, g5, g4, g3, g2, g1, g))

def p2ask(a5, a4, a3, a2, a1, a, b5, b4, b3, b2, b1, b, c5, c4, c3, c2, c1, c, d5, d4, d3, d2, d1, d, e5, e4, e3, e2, e1, e, f5, f4, f3, f2, f1, f, g5, g4, g3, g2, g1, g):
    if a5 == player1_color and a4 == player1_color and a3 == player1_color and a2 == player1_color:
        print(f"{player1} wins!")
        exit()
    if b5 == player1_color and b4 == player1_color and b3 == player1_color and b2 == player1_color:
        print(f"{player1} wins!")
        exit()
    if c5 == player1_color and c4 == player1_color and c3 == player1_color and c2 == player1_color:
        print(f"{player1} wins!")
        exit()
    if d5 == player1_color and d4 == player1_color and d3 == player1_color and d2 == player1_color:
        print(f"{player1} wins!")
        exit()
    if e5 == player1_color and e4 == player1_color and e3 == player1_color and e2 == player1_color:
        print(f"{player1} wins!")
        exit()
    if f5 == player1_color and f4 == player1_color and f3 == player1_color and f2 == player1_color:
        print(f"{player1} wins!")
        exit()
    if g5 == player1_color and g4 == player1_color and g3 == player1_color and g2 == player1_color:
        print(f"{player1} wins!")
        exit()
    if a4 == player1_color and a3 == player1_color and a2 == player1_color and a1 == player1_color:
        print(f"{player1} wins!")
        exit()
    if b4 == player1_color and b3 == player1_color and b2 == player1_color and b1 == player1_color:
        print(f"{player1} wins!")
        exit()
    if c4 == player1_color and c3 == player1_color and c2 == player1_color and c1 == player1_color:
        print(f"{player1} wins!")
        exit()
    if d4 == player1_color and d3 == player1_color and d2 == player1_color and d1 == player1_color:
        print(f"{player1} wins!")
        exit()
    if e4 == player1_color and e3 == player1_color and e2 == player1_color and e1 == player1_color:
        print(f"{player1} wins!")
